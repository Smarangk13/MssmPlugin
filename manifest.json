{
  "manifest_version": 2,

  "name": "Recipie finder",
  "description": "This extension shows the recipie for the food you looked for on google",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
